{
  "metadata": [
    {
      "src": [
        {
          //"src": "../",
          //"files": [
          //  "NetCord/NetCord.csproj",
          //  "NetCord.Services/NetCord.Services.csproj"
          //]
          //https://github.com/dotnet/docfx/issues/8097
          "src": "../",
          "files": [
            "NetCord/bin/Release/net6.0/NetCord.dll",
            "NetCord.Services/bin/Release/net6.0/NetCord.Services.dll"
          ]
        }
      ],
      "dest": "docs",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/**.yml",
          "docs/index.md"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "favicon.ico",
          "logo.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "templates/NetCord",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appFooter": "<span><strong>NetCord</strong></span>",
      "_enableNewTab": true
    },
    "xrefService": "https://xref.docs.microsoft.com/query?uid={uid}"
  }
}